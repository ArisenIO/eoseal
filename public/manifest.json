{
  "name": "Eoseal",
  "version": "1.0",
  "description": "EOS wallet!",
  "permissions": ["tabs", "activeTab", "declarativeContent", "storage", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "static/images/eoseal.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "static/images/eoseal.png",
    "32": "static/images/eoseal.png",
    "48": "static/images/eoseal.png",
    "128": "static/images/eoseal.png"
  },

  "manifest_version": 2
}
